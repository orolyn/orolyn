{
  "name": "orolyn/orolyn",
  "description": "Orolyn PHP Library",
  "type": "library",
  "require": {
    "php-64bit": "^8.1",
    "ext-eio": "*",
    "ext-ds": "*",
    "ext-pcntl": "*",
    "ext-zlib": "*",
    "ext-sockets": "*",
    "ext-ffi": "*",
    "ext-mbstring": "*",
    "symfony/console": "^6.0",
    "symfony/process": "^6.0",
    "psr/http-message": "^1.0",
    "nikic/php-parser": "^4.14"
  },
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Dominic Grostate",
      "email": "codekestrel@googlemail.com"
    }
  ],
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Orolyn\\": "src/",
      "Orolyn\\Tests\\": "tests/"
    },
    "files": ["autoload.php"]
  },
  "require-dev": {
    "vimeo/psalm": "^4.15",
    "doctrine/rst-parser": "0.5.2",
    "phpunit/phpunit": "^9.6"
  }
}
